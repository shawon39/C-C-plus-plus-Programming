#include <bits/stdc++.h>
using namespace std;

int main()
{
    string str;
    cin >> str;
    int a=0,b=0;

    for(int i=0; i<str.length(); i++)
    {
        if(str[i] == '-' ) a++;
        else b++;
    }

    if(b==0 || a % b == 0 ) cout << "YES" <endl;
    else cout << "NO" <endl;
    main();
    return 0;
}
